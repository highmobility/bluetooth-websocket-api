  "devDependencies": {
    "babelify": "^6.1.2",
    "browserify": "^10.2.4",
    "connect": "^3.4.1",
    "morgan": "^1.7.0",
    "phoenix-js": "^1.0.3",
    "serve-static": "^1.10.2",
    "watchify": "^3.7.0"
