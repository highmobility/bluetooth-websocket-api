    "WebBluetooth",
    "web-bluetooth",
    "WebSocket",
    "ble",
    "IoT"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/highmobility/bluetooth-websocket-api.git"
  },
  "bugs": {
    "url": "https://github.com/highmobility/bluetooth-websocket-api/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/highmobility/bluetooth-websocket-api/blob/master/LICENCE"
    }
