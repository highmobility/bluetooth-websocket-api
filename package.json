{
  "name": "bluetooth-websocket-api",
  "description": "Bluetooth Websocket API - a JavaScript library that provides a simple WebSocket API to find, connect and interact with Bluetooth devices.",
  "homepage": "https://www.high-mobility.com/",
  "version": "0.0.1",
  "author": {
    "name": "High-Mobility GmbH.",
    "email": "code@high-mobility.com"
  },
  "keywords": [
